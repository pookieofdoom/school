#include <stdio.h>
int main(int argc, char **argv) {
   char **cpp0;

   cpp0 = argv + 2;

   *(argv + 1) = argv[1] + 3;
   *argv[2] = *(*(cpp0 - 2) + 4);
   *(argv + 2) = *(argv + 2) + 1;
   printf("%s\n", *(argv + 2));

   *(argv + 2) = *(cpp0 - 2) + 2;
   *(cpp0 - 2) = *(argv + 2) + 1;
   printf("%s %s %s\n", *(cpp0 - 2), *(argv + 1), *(argv + 2));

   return 0;
}

