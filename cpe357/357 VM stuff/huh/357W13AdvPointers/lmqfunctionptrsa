double F1 (double first, double second)
{
   return first < second ? first:second;
}

double F2 (double first, double second)
{
   return first < second ? second:first;
}
double F3( double first, double second)
{
   double i = first, j = second;
   if ( i < 0.0 )
     i = -i;
   if ( j < 0.0 )
     j = -j;
   return i < j ? first : second;
}


double (*rule) (double, double)


while ( dim)
   best = rule(best, vals[--dim]);
