*pHead=NULL;
while(scanf("%d",&data) && data != 0){


nd = (struct Node *)malloc(sizeof(struct Node)); 
nd->data=data;
nd->next=NULL;

if(!*pHead) {
*pHead = nd;
temp = *pHead;
}
temp->next=nd;
temp=temp->next;
}

temp = *head1;
while(temp && temp->next != NULL)
{
temp = temp->next;
}
if(temp == NULL)
*head1 = *head2;
else
temp->next = *head2;
*head2 = NULL;
